{
    "plugins": ["jsdoc-plugin-typescript"],
    "typescript": {
        "moduleRoot": "src"
    },
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.ts(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
