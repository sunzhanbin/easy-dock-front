{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@styles/*": ["src/styles/*"],
      "@consts": ["src/consts/index"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"],
      "@type": ["src/type"],
      "@type/*": ["src/type/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@config": ["src/config"],
      "@config/*": ["src/config/*"],
      "@app": ["src/app"],
      "@app/*": ["src/app/*"],
      "@common/*": ["../../packages/common/*"],
      "@apis": ["src/apis"],
      "@apis/*": ["src/apis/*"]
    }
  }
}
