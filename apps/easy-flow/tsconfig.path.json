{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@styles/*": ["src/styles/*"],
      "@consts": ["src/consts/index"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"],
      "@type": ["src/type"],
      "@type/*": ["src/type/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@mock": ["src/mock"],
      "@mock/*": ["src/mock/*"],
      "@app": ["src/app"],
      "@app/*": ["src/app/*"]
    }
  }
}
