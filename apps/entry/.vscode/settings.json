{
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true,
  },
  "eslint.validate": [
    "vue",
    "javascript",
    "typescript",
    "javascriptreact"
  ],
  "eslint.options": {
    //指定vscode的eslint所处理的文件的后缀
       "extensions": [
           ".js",
           ".vue",
           ".ts",
           ".tsx"
       ]
   },
   "eslint.format.enable": true,
   "editor.formatOnSave": false,
   "editor.defaultFormatter": null,
}