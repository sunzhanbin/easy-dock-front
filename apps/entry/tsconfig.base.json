{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@http/*": [
        "src/http/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@containers/*": [
        "src/containers/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@views/*": [
        "src/views/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@common/*": [
        "../../packages/common/*"
      ],
      "@theme/*": [
        "../../packages/theme/*"
      ]
    }
  }
}
