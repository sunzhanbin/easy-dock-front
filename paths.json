{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@utils": ["src/utils"],
      "@utils*": ["src/utils*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@consts": ["src/consts"],
      "@routes": ["src/routes"],
      "@routes/*": ["src/routes/*"],
      "@layouts": ["src/layouts"],
      "@layouts/*": ["src/layouts/*"],
      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"],
      "@styles/*": ["src/styles/*"],
      "@hooks/*": ["src/hooks/*"]
    }
  }
}
